<odoo>
	<record id="brand_view_custom_form" model="ir.ui.view">
	    <field name="name">brand.view.custom.form</field>
	    <field name="model">brand.custom</field>
	    <field name="arch" type="xml">
			<form>
				<group>
					<group>
						<field name="name"/>
						<field name="company_id" invisible="1"/>
					</group>
				</group>
			</form>
        </field>
    </record>

	<record id="brand_view_custom_tree" model="ir.ui.view">
	    <field name="name">brand.view.custom.tree</field>
	    <field name="model">brand.custom</field>
	    <field name="arch" type="xml">
			<tree>
				<field name="name"/>
				<field name="company_id" invisible="1"/>
			</tree>
        </field>
    </record>

	<record id="action_brand_custom" model="ir.actions.act_window">
	   <field name="name">Brand</field>
	   <field name="res_model">brand.custom</field>
	   <field name="view_mode">tree,form</field>
	   <field name="context">{}</field>
		<field name="domain">[]</field>
<!--	   <field name="domain">[('transaction_with', '=', 'vendor')]</field>-->
<!--	   <field name="view_id" ref="customer_details_tree"/>-->
	   <field name="help" type="html">
		   <p class="o_view_nocontent_smiling_face">
			   Create Brand Here!
		   </p>
	   </field>
	</record>


	<menuitem id="menu_brand_custom"
         name="Brand"
         parent="sale.menu_sale_config"
         action="action_brand_custom"
         sequence="150"/>
</odoo>