<?xml version="1.0"?>
<odoo>
    <record id="account_move_inherit_form" model="ir.ui.view">
        <field name="name">account.move.inherit.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='to_check']" position="after">
                <field name="is_khi" invisible="1"/>
                <field name="allowed_types" invisible="1"/>
                <field name="x_journal_ids" invisible="1"/>
                <field name="x_user_id" invisible="0"/>
            </xpath>
            <xpath expr="//field[@name='payment_reference']" position="after">
                <field name="x_remarks"/>
            </xpath>
            <xpath expr="//field[@name='partner_id']" position="attributes">
                <attribute name="domain">[('is_khi', '=', is_khi),('is_company', '=', False)]</attribute>
            </xpath>
            <xpath expr="//field[@name='sale_type_id']" position="attributes">
                <attribute name="domain">[('id', 'in', allowed_types)]</attribute>
            </xpath>
            <xpath expr="//field[@name='journal_id']" position="attributes">
                <attribute name="domain">[('id', 'in', x_journal_ids)]</attribute>
            </xpath>
            <xpath expr="//tree/field[@name='product_id']" position="after">
                <field name="x_brand"/>
            </xpath>
        </field>
    </record>
</odoo>