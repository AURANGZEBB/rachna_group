<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_users_form_view_inherit_dashboard" model="ir.ui.view">
	    <field name="name">res.users.form.view.inherit.dashboard</field>
	    <field name="model">res.users</field>
	    <field name="inherit_id" ref="base.view_users_form"/>
	    <field name="arch" type="xml">
			<xpath expr="//field[@name='tz']" position="after">
				<field name="show_my_dashboard" readonly="0"/>
			</xpath>
        </field>
    </record>
</odoo>
